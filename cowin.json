	"event": [
		{
			"listen": "prerequest",
			"script": {
				"type": "text/javascript",
				"exec": [
					"var moment = require('moment');",
					"pm.environment.set('date', moment().add(1, 'day').format((\"DD-MM-YYYY\")));"
				]
			}
		},
		{
			"listen": "test",
			"script": {
				"type": "text/javascript",
				"exec": [
					"pm.test(\"18+ vaccination centers\", function () {",
					"    if (pm.response.code == 200) {",
					"        var jsonData = pm.response.json();",
					"        var centers= jsonData.centers;",
					"        var sessionAvailable = false;",
					"        centers.forEach((center,idx)=>{",
					"            center.sessions.forEach((session,sidx) =>{",
					"                if(session.min_age_limit == 18 && session.available_capacity_dose1 > 0) {",
					"                    sessionAvailable = true;",
					"                    console.log(center.name, center.address, center.district_name, session.date, session.vaccine, session.available_capacity_dose1);",
					"                }",
					"            });",
					"        });",
					"",
					"        pm.expect(sessionAvailable).to.be.true;",
					"    }",
					"    else {",
					"        pm.response.to.have.status(200);",
					"    }",
					"})"
				]
			}
		}
